{
    "license": "proprietary",
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "artesaos/seotools": "^0.20.0",
        "coderello/laravel-shared-data": "^3.0",
        "codezero/laravel-localized-routes": "^2.4",
        "codezero/laravel-unique-translation": "^3.0",
        "deployer/deployer": "^6.0",
        "doctrine/dbal": "^2.0",
        "erusev/parsedown": "^1.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/validation": "^2.4",
        "laravel-notification-channels/webhook": "^2.1",
        "laravel/fortify": "^1.5",
        "laravel/framework": "^8.12",
        "laravel/horizon": "^5.0",
        "laravel/telescope": "^4.0",
        "laravel/tinker": "^2.5",
        "okipa/laravel-bootstrap-components": "^5.0",
        "okipa/laravel-brickables": "^2.1",
        "okipa/laravel-medialibrary-ext": "^9.0",
        "okipa/laravel-stuck-jobs-notifier": "^2.0",
        "okipa/laravel-supervisor-downtime-notifier": "^2.0",
        "okipa/laravel-table": "^4.0.1",
        "plank/laravel-metable": "^4.0",
        "propaganistas/laravel-phone": "^4.0",
        "realrashid/sweet-alert": "^3.2",
        "sentry/sentry-laravel": "^2.0",
        "spatie/laravel-feed": "^3.0",
        "spatie/laravel-honeypot": "^3.0.1",
        "spatie/laravel-medialibrary": "^9.0",
        "spatie/laravel-sitemap": "^5.0",
        "spatie/laravel-translatable": "^4.6",
        "spatie/laravel-welcome-notification": "^2.1",
        "spatie/pdf-to-image": "^2.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.0",
        "beyondcode/laravel-dump-server": "^1.7",
        "brianium/paratest": "^6.2",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "nunomaduro/larastan": "^0.6",
        "phpmd/phpmd": "^2.0",
        "phpunit/phpunit": "^9.3.3",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "classmap": [
            "database/seeders",
            "database/factories"
        ],
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan horizon:publish --ansi",
            "@php artisan telescope:publish --ansi",
            "@php artisan sweetalert:publish --ansi",
            "@ide-helper"
        ],
        "ide-helper": [
            "@php artisan ide-helper:eloquent",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:model --dir=app/Models --nowrite",
            "@php artisan ide-helper:meta"
        ],
        "test": [
            "vendor/bin/phpcbf",
            "vendor/bin/phpcs",
            "vendor/bin/phpmd app,config,database,routes text phpmd.xml",
            "vendor/bin/phpstan analyse --memory-limit=-1 --error-format=table",
            "@php artisan test --parallel"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
