{
    "license": "proprietary",
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "artesaos/seotools": "^0.16.0",
        "codezero/laravel-localized-routes": "^2.1",
        "codezero/laravel-unique-translation": "^2.0",
        "deployer/deployer": "^6.0",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "^4.0",
        "laracasts/utilities": "^3.0",
        "laravel/framework": "^6.3",
        "laravel/horizon": "^3.0",
        "laravel/telescope": "^2.0",
        "laravel/tinker": "^1.0",
        "okipa/laravel-bootstrap-components": "2.0.0.x-dev",
        "okipa/laravel-failed-jobs-notifier": "^1.0",
        "okipa/laravel-medialibrary-ext": "^7.11.0",
        "okipa/laravel-request-sanitizer": "^1.1",
        "okipa/laravel-table": "^1.0",
        "plank/laravel-metable": "^2.0",
        "predis/predis": "^1.1",
        "propaganistas/laravel-phone": "^4.0",
        "realrashid/sweet-alert": "^2.0",
        "sentry/sentry-laravel": "^1.0",
        "spatie/eloquent-sortable": "^3.7",
        "spatie/laravel-sitemap": "^5.0",
        "spatie/laravel-translatable": "^4.2"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.0",
        "beyondcode/laravel-dump-server": "^1.0",
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "nunomaduro/larastan": "^0.5",
        "phpmd/phpmd": "^2.0",
        "phpunit/phpunit": "^8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-install-cmd": [
            "@php artisan ide-helper:handle"
        ],
        "post-update-cmd": [
            "@php artisan ide-helper:handle"
        ],
        "test": [
            "vendor/bin/phpcbf",
            "vendor/bin/phpcs",
            "vendor/bin/phpmd app,config,database,routes,tests text phpmd.xml",
            "vendor/bin/phpstan analyse",
            "vendor/bin/phpunit"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
